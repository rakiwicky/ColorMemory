<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rw.colormemory">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_57x57"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".InitApp"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity android:name=".ColorMemoryActivity" android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HighestScoresActivity"></activity>
    </application>

</manifest>